<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!--<item android:drawable="@mipmap/bt_btn_bg_cg_enable" android:state_enabled="true"/>-->
    <!--<item android:drawable="@mipmap/bt_btn_bg_cg_unable" android:state_pressed="false"/>-->
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="26dp" />
            <solid android:color="@color/clr_btn_press" />
            <stroke android:width="1dp"
                android:color="@color/clr_btn_stroke" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <gradient android:angle="0"
                android:endColor="@color/clr_btn_grey02"
                android:startColor="@color/clr_btn_grey01" />
            <corners android:radius="26dp" />
            <stroke android:width="1dp"
                android:color="@color/clr_btn_stroke" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient android:angle="0"
                android:endColor="@color/clr_btn_02"
                android:startColor="@color/clr_btn_01" />
            <corners android:radius="26dp" />
        </shape>
    </item>
</selector>