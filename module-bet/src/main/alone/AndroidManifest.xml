<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xtree.bet">

    <application
        android:name="com.xtree.base.debug.DebugApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/bet_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:exported="true">
        <activity android:name="com.xtree.bet.ui.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xtree.bet.ui.activity.BtDetailActivity"></activity>
        <activity android:name="com.xtree.bet.ui.activity.ExpandActivity"></activity>
    </application>
</manifest>