<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/clr_transparent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:id="@+id/ll_choose_usdt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"

        android:layout_marginLeft="@dimen/bt_text_20sp"
        android:layout_marginRight="@dimen/bt_text_20sp"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="5dp">
<!--        <item name="android:layout_width">match_parent</item>-->
<!--        <item name="android:layout_height">match_parent</item>-->
<!--        <item name="android:gravity">center</item>-->
<!--        <item name="android:padding">12dp</item>-->
<!--        <item name="android:background">@drawable/btn_acc_item_selector</item>-->
<!--        <item name="android:textColor">@color/clr_white</item>-->
        <TextView
            android:id="@+id/tv_choose_usdt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:padding="12dp"
            style="?attr/withdrawal_btn_attr"
            android:text=""
            android:textColor="@color/white"
            tools:text="提款"
            />
    </LinearLayout>

</LinearLayout>